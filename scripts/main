local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/deividcomsono/Obsidian/refs/heads/main/Library.lua"))()

Library:Notify("Loading Prism Script Hub...", 5)

local Window = Library:CreateWindow({
    Title = "Prism Script Hub",
    Footer = "v0.0.1",
    ToggleKeybind = Enum.KeyCode.RightControl,
    Center = true,
    AutoShow = true
}) 

local HomeTab = Window:AddTab("Home", "house")
local LocalPlayerTab = Window:AddTab("Local Player", "user")

local LeftGroupbox = HomeTab:AddLeftGroupbox("Settings", "wrench")

local Slider = LeftGroupbox:AddSlider("WalkspeedSlider", {
    Text = "Walkspeed",
    Default = 16,
    Min = 1,
    Max = 100,
    Rounding = 1,
    Compact = false,
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
})
